    <!--For 'Customer Identifier'-->
    <changeSet id="1" author="fineract">
        <preConditions onFail="MARK_RAN">
            <sqlCheck expectedResult="4">
                SELECT COUNT(*) FROM m_code_value
                WHERE code_id = (SELECT id FROM m_code WHERE code_name = 'Customer Identifier')
                AND order_position IN (1, 2, 3, 4)
            </sqlCheck>
        </preConditions>

        <update tableName="m_code_value">
            <column name="code_value" value="Identificador Tributario"/>
            <column name="code_description" value="RUT"/>
            <where>
                code_id = (SELECT id FROM m_code WHERE code_name = 'Customer Identifier')
                AND order_position = 1
            </where>
        </update>

        <update tableName="m_code_value">
            <column name="code_value" value="Documento Nacional Identidad"/>
            <column name="code_description" value="DNI"/>
            <where>
                code_id = (SELECT id FROM m_code WHERE code_name = 'Customer Identifier')
                AND order_position = 2
            </where>
        </update>

        <update tableName="m_code_value">
            <column name="code_value" value="Pasaporte"/>
            <column name="code_description" value=""/>
            <where>
                code_id = (SELECT id FROM m_code WHERE code_name = 'Customer Identifier')
                AND order_position = 3
            </where>
        </update>

        <update tableName="m_code_value">
            <column name="code_value" value="Permiso de Conducir"/>
            <column name="code_description" value=""/>
            <where>
                code_id = (SELECT id FROM m_code WHERE code_name = 'Customer Identifier')
                AND order_position = 4
            </where>
        </update>
    </changeSet>

